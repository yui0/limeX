+- *
+- * Image decode routine for BMP files
+- */
+-#include <stdio.h>
+-#include <stdlib.h>
+-#include <string.h>
+-#include <unistd.h>
+-#include <fcntl.h>
+-#include <ctype.h>
+-#include <sys/types.h>
