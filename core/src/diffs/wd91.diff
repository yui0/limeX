+-			*rgba = 0xff000000|(rgba_b<<16)|(rgba_g<<8)|(rgba_r);
+-		}
+-	}
+-#endif
+-	TIFFClose(tif);
+-	return 1;
+-
+-err:
+-	EPRINTF("GdDecodeTIFF: image loading error\n");
+-	if (tif)
+-		TIFFClose(tif);
+-	if(pimage->imagebits)
+-		free(pimage->imagebits);
+-	if(pimage->palette)
+-		free(pimage->palette);
+-	return 2;		/* image error*/
+-}
