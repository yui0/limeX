++	}
++	f = fopen(file->bmf_name, READ_BINARY);
++#ifdef IN_MEMORY
++#ifdef unix
++	from_pipe = FALSE;
++	if (f == NULL) {
++		char	tmp[400];
++
++		sprintf(tmp, "%s.gz", file->bmf_name);
++		if ((f = fopen(tmp, "r")) != NULL) {
++			fclose(f);
