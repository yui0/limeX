+int UC16_to_EN(const unsigned short *uc16, int cc, unsigned char *ascii)
+{
+    int i=0,j=0;
+    unsigned char ch, cl;
+    unsigned short *uc16pp;
+
+    uc16pp=(unsigned short *) uc16;
+
+    while(1){
+        if(j>=cc){
+            ascii[i]=0;
