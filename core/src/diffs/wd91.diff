-	pimage->planes = 1;
-
-	/* currently only 1, 4, 8 and 24 bpp bitmaps*/
-	if(pimage->bpp > 8 && pimage->bpp != 24) {
-		EPRINTF("GdDecodeBMP: image bpp not 1, 4, 8 or 24\n");
