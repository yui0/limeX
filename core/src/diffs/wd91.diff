+void GFX_Copy_Bitmap(GFXContext *pGFX, PSD dstpsd, int dx, int dy, int dw, int dh, PSD srcpsd, int sx, int sy, int sw, int sh, int flag, int dst_width, int src_width)
+{
+	struct RUA *pRUA = pGFX->pRUA;
