++	psd->StretchBlit = em86xx_stretchblit;
++	psd->StretchBlitEx = NULL;
++
++	/* mmap framebuffer into this address space*/
++	if (osd.buf_addr == 0) {
++		printf("Can't map framebuffer.\n");
++		goto fail;
++	}
++	psd->portrait = MWPORTRAIT_NONE;
++
++	psd->physical = psd->addr = (void *) osd.buf_addr;
