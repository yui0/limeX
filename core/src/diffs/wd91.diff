-						n = n->next;
-					}
-
-					if (!n) {
-						EPRINTF("No color found for (%s)\n", pxlstr);
-						return 0;
-					}
-
-					if (sinfo.bpp <= 8)
-						dwordcolor = (long)n->palette_entry;
-					else
-						dwordcolor = n->color;
