-					/* We grab the largest possible, and then compare */
-					strncpy(pxlstr, c, cpp);
-					z = pxlstr[0];
-
-					if (!colormap[z]) {
-						EPRINTF("No color entry for (%s)\n", pxlstr);
-						return 0;
-					}
-
-					n = colormap[z];
-
-					while (n) {
-						if (!strncmp
-						    (n->mapstr, pxlstr, cpp))
-							break;
