++#ifdef unix
++	bool	from_pipe;
++#endif
++#endif
++
++	for (fp = &(hbf->bm_file); *fp != NULL; fp = &((*fp)->bmf_next))
++		if (strcmp((*fp)->bmf_name, filename) == 0)
++			return *fp;
