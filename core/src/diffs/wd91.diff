-		/* Check it out */
-		if (xline[0] == '/' && xline[1] == '*')	/* Comment */
-			continue;
-
-		if (xline[0] != '\"')
-			continue;
-
-		/* remove the quotes from the line */
-		for (c = xline + 1, a = 0; *c != '\"' && *c != 0; c++, a++)
-			dline[a] = *c;
-
-		dline[a] = 0;
-
