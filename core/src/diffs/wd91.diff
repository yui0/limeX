-				pimage->palette[in_color].r =
-					(n->color >> 16) & 0xFF;
-				pimage->palette[in_color].g =
-					(n->color >> 8) & 0xFF;
-				pimage->palette[in_color].b = n->color & 0xFF;
-			} else {
-				if (n->color == -1) {
-					n->color = XPM_TRANSCOLOR;
-				}
-			}
