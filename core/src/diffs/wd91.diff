++static CHAR_INDEX
++b2_pos(hbf, code)
++	HBF_STRUCT	*hbf;
++	HBF_CHAR	code;
++{
++reg	B2_RANGE *b2r;
++reg	unsigned c;
++reg	CHAR_INDEX	pos;
++
++	c = SecondByte(code);
++	pos = 0;
++	for (b2r = hbf->byte_2_range; b2r != NULL; b2r = b2r->b2r_next)
++		if (b2r->b2r_start <= c && c <= b2r->b2r_finish)
++			return pos + c - b2r->b2r_start;
++		else
