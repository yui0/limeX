+-}
+-
+-static void
+-fill_input_buffer(j_decompress_ptr cinfo)
+-{
+-	return;
+-}
+-
+-static void
+-skip_input_data(j_decompress_ptr cinfo, long num_bytes)
+-{
+-	if (num_bytes >= inptr->size)
+-		return;
+-	cinfo->src->next_input_byte += num_bytes;
+-	cinfo->src->bytes_in_buffer -= num_bytes;
+-}
